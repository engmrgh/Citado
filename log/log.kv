<CustomWidthTabb@TabbedPanelItem>
    width: self.texture_size[0]
    padding: 10, 0
    size_hint_x: None

<LogPopup>:
    TabbedPanel:
        size_hint: 1,1
        do_default_tab: False
        tab_width: None

        CustomWidthTabb:
            text: 'Customer'
            on_press: root.return_query('customer_log')
            ScrollView:
                BoxLayout:
                    orientation: 'vertical'
                    id: customer_log


        CustomWidthTabb:
            text: 'Address'
            on_press: root.return_query('address_log')

            ScrollView:
                BoxLayout:
                    orientation: 'vertical'
                    id: address_log

        CustomWidthTabb:
